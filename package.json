{
  "name": "webpack-cos-plugin",
  "version": "1.1.2",
  "description": "Tencent cos-webpack auto upload files plugin ",
  "homepage": "http://www.qtrade.com.cn",
  "bugs": {
    "url": "https://github.com/sonofmagic/webpack-qcloud-cos-plugin/issues/new"
  },
  "repository": "https://github.com/sonofmagic/webpack-qcloud-cos-plugin.git",
  "license": "MIT",
  "author": "icebreaker <1324318532@qq.com>",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "docs": "cp README.md docs && ./node_modules/gh-pages/bin/gh-pages.js -d docs --silent --repo https://${GITHUB_TOKEN}@github.com/GuiGu0101/webpack-qcloud-cos-plugin.git",
    "lint": "npm-run-all --parallel lint:*",
    "lint:editconfig": "eclint check --block_comment_start '/**' --block_comment_end '*/'",
    "lint:eslint": "eslint --ext .js src --format html -o ./docs/eslint.html",
    "semantic-release": "semantic-release",
    "test": "mocha test --recursive --require babel-core/register --reporter mochawesome --reporter-options reportDir=./docs/mocha,reportName=webpack-cos-plugin,reportTitle=webpack-cos-plugin,inlineAssets=true"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "core-js": "3",
    "cos-nodejs-sdk-v5": "^2.5.12",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "babel-eslint": "^10.0.1",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "^3.1.0",
    "eclint": "^2.8.1",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^6.3.0",
    "gh-pages": "^2.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "mocha": "^7.1.1",
    "mochawesome": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^17.0.4",
    "sort-package-json": "^1.18.0"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=5.0.0"
  },
  "release": {
    "branch": "master",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          },
          "releaseRules": [
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "refactor",
              "scope": "src",
              "release": "major"
            },
            {
              "type": "perf",
              "release": "minor"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "scope",
              "subject"
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepare": "pwd && ls && git status && git diff",
          "publishCmd": "npm run docs"
        }
      ]
    ]
  }
}
